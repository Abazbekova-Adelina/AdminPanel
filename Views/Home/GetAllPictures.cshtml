@model List<News>
<h2>Добавить новую картинку</h2>

<form method="post" asp-action="Create" asp-controller="Home">
    <div>
        <h6>создание картинки</h6>
        <input type="text" name="namePicture" />
    </div>
    <div>
        <h6>создание новости</h6>
